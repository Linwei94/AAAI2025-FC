            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--cverror", "entropy",
                "--dataset", "imagenet",
                "--model-name", "swin_b",
                "--debug"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
                "WANDB_MODE": "disabled"
            }
        }
    ]
