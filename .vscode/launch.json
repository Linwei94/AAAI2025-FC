    "version": "0.2.0",
    "configurations": [
        {
            "name": "evaluate cifar10",
            "type": "python",
            "request": "launch",
            "program": "evaluate.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--cverror", "nll",
                "--dataset", "cifar10",
                "--model-name", "resnet50",
                "--saved_model_name", "resnet50_c10_cross_entropy.model"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
                "WANDB_MODE": "disabled"
            }
        },{
            "name": "evaluate imagenet",
            "type": "python",
